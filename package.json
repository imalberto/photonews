{
  "name": "photonews",
  "version": "0.0.1",
  "description": "PhotoNews web app showcasing modown components",
  "main": "app.js",
  "dependencies": {
    "connect": "2.x",
    "debug": "*",
    "es6-module-transpiler": "~0.3.4",
    "express": "3.4.x",
    "express-annotations": "~0.0.2",
    "express-map": "~0.0.3",
    "express-slash": "~0.2.1",
    "express-state": "~1.0.2",
    "express-view": "~0.0.1",
    "express-yui": "~1.0.0",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.11",
    "grunt-es6-module-transpiler": "~0.5.2",
    "locator": "~0.3.7",
    "locator-handlebars": "~0.3.0",
    "locator-yui": "0.1.x",
    "locator-react": "~0.1.0",
    "react-tools": "0.8.0",
    "yui-lint": "~0.1.3",
    "yui": "~3.17.x"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "grunt-contrib-watch": "~0.5.3",
    "istanbul": "~0.2.4",
    "jshint": "~1.1.0",
    "mocha": "~1.16.0",
    "mockery": "1.4.x",
    "ytestrunner": "0.3.x",
    "yuitest": "0.7.x"
  },
  "directories": {
    "lib": "./lib"
  },
  "engines": {
    "node": "0.10.x"
  },
  "keywords": [
    "express",
    "javascript",
    "modown",
    "photonews"
  ],
  "scripts": {
    "postinstall": "echo postinstall time; ./node_modules/grunt-cli/bin/grunt",
    "start": "./node_modules/grunt-cli/bin/grunt && node app.js",
    "lint": "jshint --config ./node_modules/yui-lint/jshint.json ./lib/**/*.js *.js",
    "cover": "./node_modules/.bin/istanbul cover -- ./node_modules/.bin/_mocha tests/test-*.js --reporter spec",
    "test": "./node_modules/mocha/bin/mocha tests/test-*.js"
  },
  "locator": {},
  "repository": "https://github.com/imalberto/photonews",
  "author": "alberto chan <imalberto@gmail.com>",
  "license": "BSD"
}
