{
  "name": "photonews",
  "version": "0.0.1",
  "description": "PhotoNews web app showcasing modown components",
  "main": "app.js",
  "dependencies": {
    "debug": "*",
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "grunt-es6-module-transpiler": "~0.5.2",
    "locator": "*",
    "locator-handlebars": "*",
    "locator-yui": "0.1.x",
    "yui": "3.x",
    "express-slash": "~0.2.1"
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-watch": "~0.5.3",
